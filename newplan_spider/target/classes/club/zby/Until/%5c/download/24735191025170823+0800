	var SDmodel = new scrollDoor();
	SDmodel.sd(["mm01","mm02","mm03","mm04","mm05","mm07","mm08"],["cc01","cc02","cc03","cc04","cc05","cc07","cc08"],"sd03","sd04");
}
</script>
